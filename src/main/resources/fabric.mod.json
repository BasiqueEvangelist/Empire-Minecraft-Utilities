  "name": "Empire Minecraft Utilities",
  "description": "A collection of various utilities for the Empire Minecraft server.",
  "authors": ["MrFrydae"],
  "contributors": ["wafflecoffee"],
  "contact": {
    "homepage": "https://github.com/MrFrydae/Empire-Minecraft-Utilities",
    "sources": "https://github.com/MrFrydae/Empire-Minecraft-Utilities",
    "issues": "https://github.com/MrFrydae/Empire-Minecraft-Utilities/issues"
  },

